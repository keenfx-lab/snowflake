{
    "name": "caneara/snowflake",
    "description": "A package to create Twitter Snowflake identifiers",
    "keywords": [
        "snowflake",
        "php",
        "laravel",
        "database"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/caneara/snowflake",
    "autoload": {
        "psr-4": {
            "Snowflake\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Snowflake\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "godruoyi/php-snowflake": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snowflake\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
